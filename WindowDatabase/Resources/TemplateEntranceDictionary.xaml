<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view ="clr-namespace:WindowDatabase.Page"
                    xmlns:viewModel ="clr-namespace:WindowDatabase.ViewModel">
    <DataTemplate DataType="{x:Type viewModel:ViewModelEntranceDatabase}">
        <view:EntranceControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:ViewModelCreateDatabase}">
        <view:CreateDatabaseControl/>
    </DataTemplate>
</ResourceDictionary>